A,CAAA,YAAA,GAAA,EAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AAhBA;AAAA;;AAAA;AAAA;AAAA;AAkBA,gBAAA,OAAA,CAAA,GAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KAtDA;AAuDA,IAAA,aAvDA,yBAuDA,OAvDA,EAuDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,sBAAA,MAAA,CAAA,IAAA,CAAA,GAAA,uBAAA,OAAA,CAAA,GAAA,EAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,WAAA;;AAHA;AAAA;;AAAA;AAAA;AAAA;AAKA,gBAAA,OAAA,CAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA9DA;AA+DA,IAAA,MA/DA,kBA+DA,IA/DA,EA+DA;AACA,aAAA,KAAA,IAAA,IAAA,IAAA,CAAA,GAAA,KAAA,KAAA,IAAA,CAAA,GAAA;AACA,KAjEA;AAkEA,IAAA,SAlEA,qBAkEA,KAlEA,EAkEA;AACA,aAAA,KAAA,WAAA,CAAA,KAAA,MAAA,MAAA;AACA,KApEA;AAqEA,IAAA,WArEA,uBAqEA,KArEA,EAqEA;AACA,UAAA,GAAA,GAAA,KAAA;AACA,UAAA,QAAA,KAAA,MAAA,QAAA,IAAA,KAAA,KAAA,IAAA,EACA,GAAA,GAAA,KAAA,CAAA,GAAA,CAHA,CAKA;;AACA,UAAA,GAAA,GAAA,EAAA,OAAA,GAAA,CAAA,KAAA,CAAA,CAAA,EAAA,OAAA,CACA,GAAA,CAAA,MAAA,GAAA,CAAA,IAAA,IADA,EACA,MADA,CAAA,CAAA;AAGA,UAAA,CAAA,GAAA,GAAA,IAAA,EAAA;AACA,UAAA,CAAA,GAAA,GAAA,IAAA,CAAA,GAAA,GAAA;AACA,UAAA,CAAA,GAAA,GAAA,GAAA,GAAA,CAXA,CAaA;;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,IAAA,CACA,SAAA,CAAA,GAAA,CAAA,IACA,SAAA,CAAA,GAAA,CAAA,CADA,GAEA,SAAA,CAAA,GAAA,CAAA,CAHA,CAAA,CAdA,CAoBA;;AACA,UAAA,GAAA,GAAA,KAAA,EAAA;AAEA,eAAA,OAAA;AACA,OAHA,MAIA;AAEA,eAAA,MAAA;AACA;AACA;AAlGA;AA9CA,CAAA","sourcesContent":["<template>\n<div>\n  <h1>Things To Do</h1>\n  <div id=\"teams\">\n    <button :style=\"{backgroundColor: team.color}\" :class=\"{ white: darkColor(team.color), selected: active(team)}\" v-for=\"team in teams\" :key=team.id @click=selectTeam(team)>{{team.name}}</button>\n  </div>\n  <form @submit.prevent=\"addTeam\">\n      <input type=\"text\" v-model=\"teamName\">\n      <swatches-picker v-model=\"color\"/>\n      <button type=\"submit\">Add a Team</button>\n    </form>\n  <div class=\"pokemons\" v-if=\"team\">\n    <p v-show=\"activePokemons.length === 0\">You can add Pokemon above!</p>\n    <form @submit.prevent=\"addPokemon\">\n      <input type=\"text\" v-model=\"text\">\n      <button type=\"submit\">Add a Pokemon</button>\n    </form>\n    <ul>\n      <li v-for=\"pokemon in filteredPokemons\" :key=\"pokemon.id\">\n        <label :class=\"{ pokemon: true, completed: item.completed }\">\n          {{ pokemon.id }}\n          {{ pokemon.name }}\n          {{ pokemon.type }}\n          {{ pokemon.image }}\n          {{ pokemon.move1 }}\n          {{ pokemon.move2 }}\n        </label>\n        <button @click=\"deletePokemon(pokemon)\" class=\"delete\">X</button>\n      </li>\n    </ul>\n  </div>\n</div>\n</template>\n\n\n\n<script>\nimport axios from 'axios';\nimport { Swatches } from 'vue-color'\nexport default {\n  name: 'Home',\n  components: {\n    'swatches-picker': Swatches\n  },\n  data() {\n    return {\n      teams: [],\n      team: null,\n      teamName: '',\n      color: \"#000\",\n      teamSpecialty: '',\n      teamVictory: null,\n\n      pokemons: [],\n      pokemon: null,\n      pokemonID: null,\n      pokemonName: '',\n      pokemonType: '',\n      pokemonImage: '',\n      pokemonMove1: '',\n      pokemonMove2: '',\n      show: 'all',\n    }\n  },\n  created() {\n    this.getTeams();\n  },\n  computed: {\n    activePokemons() {\n      return this.pokemons.filter(pokemon => {\n        return !pokemon.completed;\n      });\n    },\n    filteredPokemons() {\n      if (this.show === 'active')\n        return this.pokemons.filter(pokemon => {\n          return !pokemon.completed;\n        });\n      if (this.show === 'completed')\n        return this.pokemons.filter(pokemon => {\n          return pokemon.completed;\n        });\n      return this.pokemons;\n    },\n  },\n  methods: {\n    async addTeam() {\n      try {\n        await axios.post(\"/api/teams\", {\n          name: this.teamName,\n          color: this.color.hex,\n          specialty: this.teamSpecialty,\n          victory: this.teamVictory,\n        });\n        await this.getTeams();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getTeams() {\n      try {\n        const response = await axios.get(\"/api/teams\");\n        this.teams = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    selectTeam(team) {\n      this.team = team;\n      this.getPokemons();\n    },\n    async getPokemons() {\n      try {\n        const response = await axios.get(`/api/teams/${this.team._id}/pokemons`);\n        this.pokemons = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async addPokemon() {\n      try {\n        await axios.post(`/api/teams/${this.team._id}/pokemons`, {\n          id: this.pokemonID,\n          name: this.pokemonName,\n          type: this.pokemonType,\n          image: this.pokemonImage,\n          move1: this.pokemonMove1,\n          move2: this.pokemonMove2,\n        });\n        this.pokemonID = 0;\n        this.pokemonName = \"\";\n        this.pokemonType = \"\";\n        this.pokemonImage = \"\";\n        this.pokemonMove1 = \"\";\n        this.pokemonMove2 = \"\";\n        this.getPokemons();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deletePokemon(pokemon) {\n      try {\n        await axios.delete(`/api/teams/${this.team._id}/pokemons/${pokemon._id}`);\n        this.getPokemons();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    active(team) {\n      return (this.team && team._id === this.team._id);\n    },\n    darkColor(color) {\n      return (this.lightOrDark(color) === 'dark');\n    },\n    lightOrDark(color) {\n      let hex = color;\n      if (typeof color === 'object' && color !== null)\n        hex = color.hex;\n\n    // Convert it to RGB: http://gist.github.com/983661\n      let rgb = +(\"0x\" + hex.slice(1).replace( \n      hex.length < 5 && /./g, '$&$&'));\n\n      const r = rgb >> 16;\n      const g = rgb >> 8 & 255;\n      const b = rgb & 255;\n      \n      // HSP (Highly Sensitive Poo) equation from http://alienryderflex.com/hsp.html\n      const hsp = Math.sqrt(\n      0.299 * (r * r) +\n      0.587 * (g * g) +\n      0.114 * (b * b)\n      );\n\n      // Using the HSP value, determine whether the color is light or dark\n      if (hsp>127.5) {\n\n          return 'light';\n      } \n      else {\n\n          return 'dark';\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* List */\nul {\n  list-style: none;\n}\n\nli {\n  background: #fff;\n  width: 500px;\n  min-height: 40px;\n  padding: 10px;\n  margin-bottom: 10px;\n  font-size: 1em;\n  display: flex;\n  align-items: center;\n}\n\n.delete {\n  display: none;\n  margin-left: auto;\n}\n\nli:hover .delete {\n  display: block;\n}\n\nlabel {\n  width: 400px;\n}\n\n.completed {\n  text-decoration: line-through;\n}\n\n/* Form */\ninput[type=checkbox] {\n  transform: scale(1.5);\n  margin-right: 10px;\n}\n\ninput[type=text] {\n  font-size: 1em;\n}\n\n.vc-swatches {\n  margin-bottom: 20px;\n}\n\n#projects {\n  margin-bottom: 20px;\n}\n\n\n\nbutton {\n  font-family: 'Arvo';\n  font-size: 1em;\n  border: none;\n  background-color: #bbb;\n  padding: 5px 10px;\n}\n\nbutton:focus {\n  outline: none;\n}\n\nbutton.selected {\n  border-bottom: 2px solid #000;\n}\n\n.white {\n  color: #fff;\n}\n\n/* Controls */\n.controls {\n  margin-top: 20px;\n}\n\n/* Custom checkbox\n/* Customize the label (the container) */\n.item {\n  display: block;\n  position: relative;\n  padding-left: 35px;\n  margin-bottom: 12px;\n  cursor: pointer;\n  font-size: 22px;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n}\n\n/* Hide the browser's default checkbox */\n.item input {\n  position: absolute;\n  opacity: 0;\n  cursor: pointer;\n  height: 0;\n  width: 0;\n}\n\n/* Create a custom checkbox */\n.checkmark {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 25px;\n  width: 25px;\n  background-color: #eee;\n}\n\n/* On mouse-over, add a grey background color */\n.item:hover input~.checkmark {\n  background-color: #ccc;\n}\n\n/* When the checkbox is checked, add a blue background */\n.item input:checked~.checkmark {\n  background-color: #2196F3;\n}\n\n/* Create the checkmark/indicator (hidden when not checked) */\n.checkmark:after {\n  content: \"\";\n  position: absolute;\n  display: none;\n}\n\n\n</style>\n"],"sourceRoot":"src/views"}]}